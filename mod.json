{
	"geode": "4.7.0",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074",
		"android": "2.2074"
	},
	"id": "raydeeux.mrjimboree",
	"name": "JimSaysYouCanDoIt",
	"version": "v1.0.0",
	"developers": ["RayDeeUx", "Nwo5"],
	"description": "you can do it!!! (Shows an image every time you're near your best score.)",
	"repository": "https://github.com/RayDeeUx/YouCanDoIt",
	"settings": {
		"enabled": {
			"name": "Enabled",
			"description": "Toggles the mod.",
			"type": "bool",
			"default": true
		},
		"previewjim": {
			"name": "Click me to preview Jim!",
			"type": "custom:previewjim"
		},
		"useBestPercentage": {
			"name": "Use Best Percentage to Trigger Jim",
			"description": "Use the level's best percentage to show Jim.",
			"type": "bool",
			"default": true,
			"enable-if": "enabled"
		},
		"alternativePercentage": {
			"name": "Alternative Percentage",
			"description": "If the  `Use Best Percentage to Trigger Jim`  setting is not enabled, choose the percent you need to reach to show Mr. Jim M. Boree.",
			"type": "float",
			"default": 5.00,
			"min": 5.00,
			"max": 100.0,
			"enable-if": "enabled && !useBestPercentage"
		},
		"percentageThreshold": {
			"name": "Percentage Threshold",
			"description": "Threshold (in percent) necessary to show Mr. Jim M. Boree.",
			"type": "float",
			"default": 0.50,
			"min": 0.00,
			"max": 100.0,
			"enable-if": "enabled"
		},
		"image": {
			"name": "Image",
			"description": "The image you want to show (in case you dislike Mr. Jim M. Boree, for whatever reason).\n\n<c-ffffff>JPG/JPEG files are not allowed.</c>",
			"type": "file",
			"default": "{mod_resources_dir}/youCanDoIt.png",
			"control": {
				"filters": [
					{
						"description": "Image files",
						"files": [
							"*.png",
							"*.gif"
						]
					}
				]
			},
			"enable-if": "enabled"
		},
		"moveForDuration": {
			"name": "Move for... (seconds)",
			"description": "Duration of the Mr. Jim M. Boree animation. <cy>Does not affect playback speed of optional SFX.</c>",
			"type": "float",
			"default": 0.50,
			"min": 0.10,
			"max": 10.0,
			"enable-if": "enabled"
		},
		"startMovingFrom": {
			"name": "Start Moving From...",
			"description": "The side of the screen you want the image to start moving from.",
			"type": "string",
			"default": "Right",
			"one-of": [
				"Right",
				"Left",
				"Top",
				"Bottom",
				"Top Left",
				"Top Right",
				"Bottom Left",
				"Bottom Right"
			],
			"enable-if": "enabled"
		},
		"moveToEasingType": {
			"name": "\"Moving In\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"moveToEasingRate": {
			"name": "\"Moving In\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"initialScale": {
			"name": "Initial Scale",
			"type": "float",
			"default": 1.00,
			"min": 0.00,
			"max": 1.00,
			"enable-if": "enabled"
		},
		"scaleToEasingType": {
			"name": "\"Scaling In\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"scaleToEasingRate": {
			"name": "\"Scaling In\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"initialRotation": {
			"name": "Initial Rotation",
			"type": "float",
			"default": 0.000,
			"min": 0.000,
			"max": 360.0,
			"enable-if": "enabled"
		},
		"rotateToEasingType": {
			"name": "\"Rotating In\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"rotateToEasingRate": {
			"name": "\"Rotating In\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"initialColor": {
			"name": "Initial Color",
			"description": "Why would you want to color Mr. Jim M. Boree? Mr. Jim M. Boree is already perfect.",
			"type": "color",
			"default": "#FFFFFF",
			"enable-if": "enabled"
		},
		"initialOpacity": {
			"name": "Initial Opacity",
			"type": "int",
			"default": 255,
			"min": 0,
			"max": 255,
			"enable-if": "enabled"
		},
		"moveToX": {
			"name": "Move to... (X Position) (%)",
			"type": "int",
			"default": 50,
			"min": 0,
			"max": 100,
			"enable-if": "enabled"
		},
		"moveToY": {
			"name": "Move to... (Y Position) (%)",
			"type": "int",
			"default": 65,
			"min": 0,
			"max": 100,
			"enable-if": "enabled"
		},
		"scaleTo": {
			"name": "Scale To...",
			"type": "float",
			"default": 0.75,
			"min": 0.50,
			"max": 1.00,
			"enable-if": "enabled"
		},
		"colorTo": {
			"name": "Color To...",
			"description": "Why would you want to color Mr. Jim M. Boree? Mr. Jim M. Boree is already perfect.",
			"type": "color",
			"default": "#FFFFFF",
			"enable-if": "enabled"
		},
		"holdForDuration": {
			"name": "Hold for... (seconds)",
			"description": "How long Mr. Jim M. Boree should be on screen before fading away. <cy>Does not affect playback speed of optional SFX.</c>",
			"type": "float",
			"default": 0.50,
			"min": 0.10,
			"max": 5.00,
			"enable-if": "enabled"
		},
		"returnToDuration": {
			"name": "Move away for... (seconds)",
			"description": "How long Mr. Jim M. Boree should be on screen before fading away. <cy>Does not affect playback speed of optional SFX.</c>",
			"type": "float",
			"default": 1.00,
			"min": 0.10,
			"max": 10.0,
			"enable-if": "enabled"
		},
		"moveBackToEasingType": {
			"name": "\"Moving Back\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"moveBackToEasingRate": {
			"name": "\"Moving Back\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"scaleBackToEasingType": {
			"name": "\"Scaling Back\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"scaleBackToEasingRate": {
			"name": "\"Scaling Back\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"rotateBackToEasingType": {
			"name": "\"Rotating Back\" Easing Type",
			"type": "string",
			"default": "None (Linear)",
			"one-of": [
				"None (Linear)",
				"Ease In", "Ease Out", "Ease In Out",
				"Back In", "Back Out", "Back In Out",
				"Bounce In", "Bounce Out", "Bounce In Out",
				"Elastic In", "Elastic Out", "Elastic In Out",
				"Exponential In", "Exponential Out", "Exponential In Out",
				"Sine In", "Sine Out", "Sine In Out"
			],
			"enable-if": "enabled"
		},
		"rotateBackToEasingRate": {
			"name": "\"Rotating Back\" Easing Rate",
			"type": "float",
			"default": 0.60,
			"min": 0.10,
			"max": 4.00,
			"enable-if": "enabled"
		},
		"sfx": {
			"name": "SFX",
			"description": "The SFX you want to play in addition to the image (in case you want it, for whatever reason).",
			"type": "file",
			"default": "",
			"control": {
				"filters": [
					{
						"description": "Audio files",
						"files": [
							"*.mp3",
							"*.oga",
							"*.ogg",
							"*.wav",
							"*.flac"
						]
					}
				]
			},
			"enable-if": "enabled"
		},
		"volume": {
			"name": "SFX Volume (%)",
			"description": "The volume of the SFX, if you've selected one already.",
			"type": "int",
			"default": 50,
			"min": 0,
			"max": 100,
			"enable-if": "enabled"
		},
		"logging": {
			"name": "Logging",
			"type": "bool",
			"default": false,
			"enable-if": "enabled"
		}
	},
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"tags": [
		"customization", "joke", "gameplay"
	],
	"dependencies": {
		"prevter.imageplus": ">=1.0.3"
	},
